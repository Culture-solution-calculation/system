<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<Tab xmlns="http://javafx.com/javafx"
     xmlns:fx="http://javafx.com/fxml"
     fx:controller="org.main.culturesolutioncalculation.MicroResultTabController"
     fx:id="microResultTab"
     text="미량원소(계산결과)">
    <content>
        <VBox alignment="CENTER" spacing="10">
            <stylesheets>
                <URL value="@style/application.css" />
            </stylesheets>

            <TableView fx:id="tableView" minHeight="500" />

            <HBox alignment="CENTER" spacing="10">
                <Button text="이전" onAction="#prevButton" />
                <Button text="다음" onAction="#nextButton" />
            </HBox>
        </VBox>
    </content>
</Tab>
